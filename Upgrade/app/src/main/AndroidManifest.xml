<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zlove.upgrade">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".rx.CreateActivity"/>
        <activity android:name=".rx.JustActivity"/>
        <activity android:name=".rx.FromActivity"/>
        <activity android:name=".rx.DeferActivity"/>
        <activity android:name=".rx.IntervalActivity"/>
        <activity android:name=".rx.TimerActivity"/>
        <activity android:name=".rx.RangeActivity"/>
    </application>

</manifest>